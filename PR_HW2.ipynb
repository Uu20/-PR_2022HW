    "from tensorflow.keras import models\n",
    "from tensorflow.keras import layers\n",
    "\n",
    "#總共有2層的CNN，CNN做完，都需要做pooling\n",
    "#最後做softmax，選擇辨識最高的當作答案\n",
    "model = models.Sequential(\n",
